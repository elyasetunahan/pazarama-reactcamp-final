{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "prettier"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
        "prettier/prettier" : ["error"],
        "react/jsx-props-no-spreading": "off",
        "no-param-reassign": 0,
        "react/button-has-type" : "off",
        "no-alert": "off"
    }
}
